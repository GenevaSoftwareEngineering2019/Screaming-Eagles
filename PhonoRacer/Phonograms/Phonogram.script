function init(self)
	msg.post(".", "acquire_input_focus") 
end

function on_input(self, action_id, action)
		local pos = go.get_position()
		pos.x = pos.x - 100 
		local Phonogram = factory.create("#factory", pos) 
		go.animate(Phonogram, "position.x", go.PLAYBACK_ONCE_FORWARD, 720, go.EASING_LINEAR, 3) 
	end
